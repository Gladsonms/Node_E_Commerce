    function signOut() {

        console.log("signout triggered")
        var auth2 = gapi.auth2.getAuthInstance();
        auth2.signOut().then(function () {
            console.log('User signed out.');
        });
    }
        
		$('#searchproduct').on("submit",(e)=>{
			e.preventDefault();
			$.ajax({
				url:"/productsearch",
				method:"POST",
				data:$("#searchproduct").serialize(),
				success:(response)=>{
					console.log(response);
				   document.getElementById('allproducts').style.display ="none";
				   
				   let sercheditem;
				    console.log(response.serach.length)
				   for(i=0;i<response.serach.length;i++) {
					   sercheditem=`<div class="col-md-3 col-sm-4 col-xs-12">
													
													<div class="product-item">
														<div class="product-image">
															<a href="/productdetails/${response.serach[i]._id}">
																<img class="img-responsive" src="/product-images/product-image1/${response.serach[i]._id}.jpg" alt="Product Image" >
															</a>
														</div>
														
														<div class="product-title">
															<a href="#">
																${response.serach[i].product}
															</a>
														</div>
														
														<div class="product-rating">
															<div class="star on"></div>
															<div class="star on"></div>
															<div class="star on "></div>
															<div class="star on"></div>
															<div class="star"></div>
														</div>

														{{#if this.productOffer}}
															<div class="product-priccsczse">
																<del class="sale-price">₹${response.serach[i].price}</del>
																<span class="base-price">${response.serach[i].productofferprice}</span>
															</div>

														{{else}}
														<div class="product-priccsczse">
															<span class="base-price">₹${response.serach[i].price}</span>
														</div>

														{{/if}}
														
														
														
														<div class="product-buttoncnxlkc" style="margin-top: 1.3em;">
												
                                                                 
                                                            <button class="btn btn-primary" onclick="addToCart('${response.serach[i]._id}')">
